{
    "name": "angular2-authorization",
    "version": "1.0.0",
    "description": "Authorization and authentication module for angular2",
    "scripts":
    {
        "clean": "rimraf ./*.d.ts ./*.js ./*.map",
        "clean:modules": "rimraf ./node_modules",
        "clean:all": "npm run clean && npm run clean:modules",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "lite": "lite-server",
        "start": "concurrent \"npm run tsc:w\" \"npm run lite\" "
    },
    "repository":
    {
        "type": "git",
        "url": "git+https://gitlab.com/Buko/angular2-authorization.git"
    },
    "bugs":
    {
        "url": "https://gitlab.com/Buko/angular2-authorization/issues"
    },
    "homepage": "https://gitlab.com/Buko/angular2-authorization#README",
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "angular2": "2.0.0-beta.1",
        "systemjs": "0.19.6",
        "es6-promise": "^3.0.2",
        "es6-shim": "^0.33.3",
        "reflect-metadata": "0.1.2",
        "rxjs": "5.0.0-beta.0",
        "zone.js": "0.5.10"
    },
    "devDependencies":
    {
        "concurrently": "^1.0.0",
        "typescript": "^1.7.5",
        "rimraf": "~2.5.1"
    }
}