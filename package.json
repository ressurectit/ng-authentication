{
    "name": "@ng2/authentication",
    "version": "1.0.0",
    "description": "Angular 2 module used for handling authentication",
    "main": "index.js",
    "typings": "index.d.ts",
    "scripts":
    {
        "prebuild": "typings install",
        "build": "npm run tsc",
        "clean": "rimraf ./*.d.ts ./index.js ./index.dev.js ./index.min.js ./*.map ./*.log ./common",
        "clean:deps": "rimraf ./node_modules ./typings",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "tsc:w": "tsc -w --sourceMap",
        "tsc:debug": "tsc --sourceMap",
        "prepublish": "npm run bundle:package",
        "postpublish": "npm run clean",
        "republish": "npm unpublish --force && npm publish",
        "bundle": "tsc --outFile ./index.dev.js",
        "bundle:update": "nab ./index.dev.js && uglifyjs -o ./index.min.js -- ./index.dev.js",
        "bundle:package": "npm run clean && npm run build && npm run bundle && npm run bundle:update",
        "deploy-stable": "npp -b",
        "deploy-next": "npp -p -b -s \"beta\" -t \"next\""
    },
    "keywords": [
        "angular2",
        "ng2",
        "authentication"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "dependencies":
    {
        "@angular/core": "2.0.0-rc.2",
        "@angular/compiler": "2.0.0-rc.2",
        "@angular/common": "2.0.0-rc.2",
        "@angular/platform-browser": "2.0.0-rc.2",
        "@angular/platform-browser-dynamic": "2.0.0-rc.2",
        "@angular/router": "3.0.0-alpha.7",
        "es6-promise": "^3.1.2",
        "es6-shim": "^0.35.0",
        "reflect-metadata": "0.1.3",
        "zone.js": "0.6.12",
        "rxjs": "5.0.0-beta.6"
    },
    "devDependencies":
    {
        "rimraf": "~2.5.2",
        "typescript": "1.8.10",
        "typings":"^0.7.12",
        "uglify-js": "~2.6.2",
        "npm-publish-package": "1.2.0",
        "npm-angular2-bundle": "1.0.1"
    }
}